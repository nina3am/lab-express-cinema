<div class="list">
    <h1>Movies list</h1>
    <div class="movies-list">
        {{#each movies}}
        <div class="movie-block">
            <h2>{{this.title}}</h2>
            <img src="{{this.image}}">
            <p><a href="/movie/{{id}}">See more</a></p>
        </div>
        {{/each}}
    </div>
</div>